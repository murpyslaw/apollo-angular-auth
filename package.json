{
  "name": "apollo-angular-auth",
  "version": "1.0.0",
  "description": "simple client / server example using apollo-client and apollo-server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-frontend": "cd apollo-angular-auth-client && npm start",
    "start-backend": "cd apollo-angular-auth-server && npm start",
    "install": "cd apollo-angular-auth-client && npm i && cd ../apollo-angular-auth-server && npm i",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\""
  },
